{
	"name": "mqt-movers-calendar-watcher",
	"version": "1.0.0",
	"description": "a node serverless function that hits a google calendar on a regular interval to check for added events quantity",
	"main": "index.ts",
	"scripts": {
		"start": "nodemon",
		"offline": "npm run build:lambda && serverless offline",
		"dev": "npm run build:lambda && netlify dev",
		"build:lambda": "npm run bundle && tsc -p lambda/tsconfig.json",
		"bundle": "ncp ./src ./lambda/bundle"
	},
	"author": "brahm van houzen",
	"license": "MIT",
	"dependencies": {
		"@as-integrations/aws-lambda": "^3.1.0",
		"@google-cloud/local-auth": "^2.1.0",
		"dotenv": "^16.4.5",
		"express": "^4.21.1",
		"googleapis": "^105.0.0",
		"ncp": "^2.0.0",
		"nodemailer": "^6.9.15",
		"serverless-http": "^3.2.0",
		"typescript": "^5.6.3"
	},
	"devDependencies": {
		"@types/express": "^5.0.0",
		"@types/node": "^22.7.5",
		"@types/nodemailer": "^6.4.16",
		"eslint": "^9.12.0",
		"nodemon": "^3.1.7",
		"ts-node": "^10.9.2"
	}
}
